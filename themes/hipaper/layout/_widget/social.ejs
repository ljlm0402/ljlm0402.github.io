<% if (theme.social){ %>
  <div class="widget-wrap">
    <h3 class="widget-title"><%= __('connect') %></h3>
    <div class="widget widget_athemes_social_icons">

    	<ul class="clearfix widget-social-icons">   
		<% Object.keys(theme.social).forEach(function(key) { %>
			   <li><a href="<%= theme.social[key] %>" title="<%=key.trim()%>"><i class="icon-<%= key.toLowerCase() %>" aria-hidden="true"></i></a></li> 
   		<% }) %>
   		</ul>
    </div>
  </div>
<% } %>
